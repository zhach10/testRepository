<apex:page standardController="dsfs__DocuSign_Envelope__c"
	Extensions="dsfs.DocuSignEnvelopeController" tabStyle="dsfs__DocuSign_Status__c"
	standardStylesheets="true" ID="SendingCompleted">
	<style type="text/css">
		#returnButton{
			position: absolute;			
			left: 600px;
			top: 15px;
		}
	</style>
	<apex:form >
		<apex:pageBlock title="{!CompleteMessage}" id="thePageBlock" mode="Edit" tabStyle="DocuSign_Status__c">
			<div id="returnButton">
				<apex:commandButton value="{!$Label.dsfs__Return}" action="{!DSReturnToSource}"/>
			</div>			
			<apex:actionRegion >
				<apex:pageBlockSection title="{!$Label.dsfs__EnvelopeInfo}" columns="1" collapsible="false" showHeader="false">
					<apex:pageBlockSectionItem >
						<apex:image url="{!$Resource.dsfs__DocuSignLogo}" title="{!$Label.dsfs__SendComplete}."/>
						<apex:outputpanel >
							<table cellpadding="3" cellspacing="0" border="0" style="font-size: 8pt">
								<tr>
									<td align="right" style="white-space: nowrap; font-weight: bold">
										<apex:outputLabel value="{!$Label.dsfs__TransctionInfo}"/>
									</td>
									<td>
										<apex:outputLabel value="{!mySource}"/>
									</td>
								</tr>
								<tr>
									<td align="right" style="white-space: nowrap; font-weight: bold">
										<apex:outputLabel value="{!$Label.dsfs__EnvelopeID}"/>
									</td>
									<td>
										<apex:outputLabel value="{!DSEnvelope.dsfs__DocuSign_Envelope_ID__c}"/>
									</td>
								</tr>
								<tr>
									<td align="right" style="white-space: nowrap; font-weight: bold">
										<apex:outputLabel value="{!$Label.dsfs__Status}"/>
									</td>
									<td>
										<apex:outputLabel value="{!MyTaggerResult}"/>
									</td>
								</tr>
								<tr>
									<td align="right" style="white-space: nowrap; font-weight: bold">
										<apex:outputLabel value="{!$Label.dsfs__SignNowStatus}" rendered="{!IsSignNowStatusPage}"/>
									</td>
									<td>
										<apex:outputLabel value="{!MySignNowResult}" rendered="{!IsSignNowStatusPage}"/>
									</td>
								</tr>
							</table>							
						</apex:outputpanel>
					</apex:pageBlockSectionItem>
				</apex:pageBlockSection>
			</apex:actionRegion>
		</apex:pageBlock>
		<c:DocuSignAppExchangeInvite />
	</apex:form>
</apex:page>